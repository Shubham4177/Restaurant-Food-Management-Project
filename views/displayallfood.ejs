<style>.maincontainer{
    height: 100vh; 
    width: 100vw; 
    background: #95afc0;
    display: flex;
    justify-content: center;
    align-items: center;

 }

 .box{
     width: 80vw;
     height: 70vh;
     background: white;
     border-radius: 15px;
     padding: 20px;
     overflow: scroll;

 }

 .headingdiv{
     padding: 5px;
     display: flex;
     align-items: center;
     justify-content: space-between;
 }
</style>
<html>
    <title>FOOD LIST</title>
    <head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js" integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.min.js" integrity="sha384-mQ93GR66B00ZXjt0YO5KlohRA5SY2XofN4zfuZxLkoj1gXtW8ANNCe9d5Y3eG5eD" crossorigin="anonymous"></script>
<script src=" https://code.jquery.com/jquery-3.6.3.min.js"></script>
<script src="/javascripts/project.js"></script>

    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Lato:ital@1&family=Poppins:wght@300&display=swap" rel="stylesheet">
</head>
<div class="maincontainer">
    <div class="box">
        <div class="headingdiv">
            <div>
            <img src="/images/foodlogo.png" width="30" height="30"/>
            <span style="font-size: 16px; font-weight: bold; font-family: poppins; letter-spacing: 1;padding-left: 10px;"> Food List</span></div>
            <div><a href="/food/foodinterface" style="text-decoration: none
            ;"><img src="/images/foodrecordicon.png" width="30" height="30"/></a></div>
        </div>
<table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">FOOD ID</th>
        <th scope="col">FOOD Cuisine</th>
        <th scope="col">FOOD ITEM</th>
        <th scope="col">STATUS TYPE</th>
        <th scope="col">STATUS</th>
        <th scope="col">DESCRIPTION</th>
        <th scope="col">Price <br>Offer</th>
        <th scope="col">ADDITIONAL</th>
        <th scope="col">POSTER</th>
        <th scope="col">Edit/Delete</th>
      </tr>
    </thead>
    <tbody>
        <% data.map((item)=>{ %>
            <tr>

        
        <th scope="row"><%= item.fdid %></th>
        <td><%= item.foodcuisinename %></td>
        <td><%= item.fooditem %></td>
        <td><%= item.statustype %></td>
        <td><%= item.status %></td>
        <td><%= item.description %></td>
        <td><s><%= item.price %></s><br><%= item.offer %></td>
        <td><%= item.additional %></td>
        <td><a href="/food/displayposter?fdid=<%=item.fdid %>&fooditem=<%=item.fooditem %>&poster=<%= item.poster %>"><img src="/images/<%= item.poster %>" style="width: 50px;height: 50px; border-radius: 10px;"  ></a></td>
        <td><a href="/food/edit_food_data?fdid=<%= item.fdid %>" style="text-decoration: none; font-weight: 700;"> Edit/Delete</a></td>
      </tr>
    <% }) %>
    </tbody>
  </table>
</div>
</div>

</html>